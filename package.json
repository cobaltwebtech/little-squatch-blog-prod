{
  "name": "littlesquatchblog",
  "type": "module",
  "version": "2.0.0",
  "description": "The Little Squatch blog built with Astro",
  "author": "cobaltwebtech",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/cobaltwebtech/little-squatch-blog-prod",
    "private": true
  },
  "packageManager": "pnpm@10.27.0",
  "scripts": {
    "dev": "astro dev",
    "start": "astro dev",
    "build": "astro build",
    "upforce": "pnpm up --latest --recursive '*'",
    "up:astro": "pnpx @astrojs/upgrade",
    "up:biome": "pnpm up @biomejs/biome@latest && pnpm biome migrate --write",
    "format": "biome format --write",
    "lint": "biome lint",
    "clean": "rimraf dist .astro node_modules pnpm-lock.yaml"
  },
  "dependencies": {
    "@astrojs/mdx": "4.3.13",
    "@astrojs/netlify": "^6.6.3",
    "@astrojs/rss": "^4.0.14",
    "@astrojs/sitemap": "3.6.0",
    "@playform/compress": "^0.2.0",
    "@tailwindcss/vite": "^4.1.18",
    "astro": "5.16.6",
    "astro-cloudinary": "^1.3.5",
    "astro-compressor": "^1.2.0",
    "astro-font": "^1.1.0",
    "astro-icon": "^1.1.5",
    "clsx": "^2.1.1",
    "lenis": "^1.3.17",
    "mdast-util-to-string": "^4.0.0",
    "photoswipe": "^5.4.4",
    "preline": "^3.2.3",
    "reading-time": "^1.5.0",
    "slugify": "^1.6.6",
    "tailwind-merge": "^3.4.0",
    "tailwind-variants": "^1.0.0",
    "tailwindcss": "^4.1.18",
    "tiny-invariant": "^1.3.3",
    "tw-animate-css": "^1.4.0"
  },
  "devDependencies": {
    "@biomejs/biome": "^2.3.11",
    "@tailwindcss/forms": "^0.5.11",
    "@tailwindcss/typography": "^0.5.19",
    "rimraf": "^6.1.2",
    "typescript": "^5.9.3"
  }
}
